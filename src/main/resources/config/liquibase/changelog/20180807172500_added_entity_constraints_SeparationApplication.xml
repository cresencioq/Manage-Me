<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity SeparationApplication.
    -->
    <changeSet id="20180807172500-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="employee_id"
                                 baseTableName="separation_application"
                                 constraintName="fk_separation_application_employee_id"
                                 referencedColumnNames="id"
                                 referencedTableName="employee"/>

        <addForeignKeyConstraint baseColumnNames="separation_applications_id"
                                 baseTableName="separation_application_functional_representative"
                                 constraintName="fk_separation_application_functional_representative_separation_applications_id"
                                 referencedColumnNames="id"
                                 referencedTableName="separation_application"/>
        <addForeignKeyConstraint baseColumnNames="functional_representatives_id"
                                 baseTableName="separation_application_functional_representative"
                                 constraintName="fk_separation_application_functional_representative_functional_representatives_id"
                                 referencedColumnNames="id"
                                 referencedTableName="employee"/>
        
    </changeSet>
</databaseChangeLog>
